   "source": [
    "class Solution:\n",
    "    def climbStairs(self, n):\n",
    "        \n",
    "        if (n == 1) | (n == 2):\n",
    "            return n\n",
    "        a, b = 1, 2\n",
    "        i = 3\n",
    "        while i <= n:\n",
    "            a, b = b, a + b\n",
    "            i += 1\n",
    "        \n",
    "        return b"
   ]
  },
